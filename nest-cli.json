{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "assets": [
      { "include": "**/*.hbs", "watchAssets": true },
      { "include": "**/database/migration/seeders/*.js", "watchAssets": true }
    ]
  }
}
